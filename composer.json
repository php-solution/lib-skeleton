{
    "name": ":vendor/:package_name",
    "type": "component",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0"
    },
    "suggest": {
    },
    "autoload":     {
        "psr-4": { ":vendor\\:package_name\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { ":vendor\\:package_name\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}